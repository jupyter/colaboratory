{
  "name": "Colaboratory Notebook",
  "version": "1",
  "manifest_version": 2,
  "description": "A Chrome App for running an IPython backend in a notebook environment with Google Drive Integration.  The backend comes packaged with the app and is based on a PNaCl port of IPython.",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "key": "",
  "oauth2": {
    "client_id": "911569945122-kc8fl47230hka61bigmft8sl84btshtl.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "permissions": [
  "identity",
	{"socket": [
        "tcp-listen:*:*",
        "tcp-connect",
        "resolve-host",
        "udp-bind:*:*",
        "udp-send-to:*:*"
      ]
  },
	"webview",
	"storage",
	"clipboardRead",
	"clipboardWrite"
  ],
  "url_handlers": {
    "handle_drive_action": {
      "matches": [ "http://127.0.0.1:8000/drive*" ],
      "title": "Drive Action"
    }
  }
}
