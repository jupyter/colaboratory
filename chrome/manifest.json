{
  "name": "coLaboratory Notebook",
  "version": "6",
  "manifest_version": 2,
  "description": "A Chrome App for running a port of IPython to PNaCl, in a notebook environment with Google Drive Integration.",
  "icons": {
    "256": "colab-black-256.png"
  },
  "app": {
    "background": {
      "scripts": [
        "app/params.js",
        "app/launch.js",
        "app/main.js"
      ]
    }
  },
  "oauth2": {
    "client_id": "911569945122-0tcrnl8lnu5b0ccgpp92al27pplahn5a.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/drive.install"
    ]
  },
  "permissions": [
    "identity",
    {"socket": [
        "tcp-listen:*:*",
        "tcp-connect",
        "resolve-host",
        "udp-bind:*:*",
        "udp-send-to:*:*"]
    },
    "webview",
    {"fileSystem" : ["write", "directory", "retainEntries"]},
    "unlimitedStorage",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    {"fileSystem": [
        "write",
        "retainEntries",
        "directory"]
    }
    ],
  "url_handlers": {
    "handle_drive_action": {
      "matches": [ "http://colaboratory.jupyter.org/drive*" ],
      "title": "Drive Action"
    }
  },
  "webview": {
    "partitions" :[
      {
        "name": "frontend",
        "accessible_resources": ["notebook/*", "closure/*", "colab/*", "extern/*", "ipython/*"]
      }
    ]
  }
}
