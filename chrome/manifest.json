{
  "name": "Colaboratory Notebook",
  "version": "6",
  "manifest_version": 2,
  "description": "A Chrome App for running a port of IPython to PNaCl, in a notebook environment with Google Drive Integration.",
  "app": {
    "background": {
      "scripts": [
        "launch.js",
        "main.js"
      ]
    }
  },
  "key": "",
  "oauth2": {
    "client_id": "564937920753-2t209pu2ig5eh6f5fk7bb8mteole78b6.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/drive.install"
    ]
  },
  "permissions": [
    "identity",
    {"socket": [
        "tcp-listen:*:*",
        "tcp-connect",
        "resolve-host",
        "udp-bind:*:*",
        "udp-send-to:*:*"]
    },
    "webview",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    {"fileSystem": [
        "write",
        "retainEntries",
        "directory"]
    }
    ],
  "url_handlers": {
    "handle_drive_action": {
      "matches": [ "http://iteractivenb.appspot.com/drive*" ],
      "title": "Drive Action"
    }
  },
  "webview": {
    "partitions" :[
      {
        "name": "frontend",
        "accessible_resources": ["static/*"]
      }
    ]
  }
}
